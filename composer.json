{
    "name": "projectinfiniteme/laravel-core-translations",
    "description": "Core translations microservice",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yure Nery",
            "email": "yurenery@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^8.0",
        "projectinfiniteme/laravel-core-classes": "dev-main",
        "laravel/passport": "^11.0",
        "spatie/laravel-translation-loader": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "AttractCores\\LaravelCoreTranslation\\": "src/"
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\": "database/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AttractCores\\LaravelCoreClasses\\CoreControllerServiceProvider",
                "AttractCores\\LaravelCoreTranslation\\CoreTranslationServiceProvider"
            ]
        }
    }
}
